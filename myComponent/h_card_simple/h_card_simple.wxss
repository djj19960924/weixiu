.cardContainer{
  width: 100%;
  display: flex;
  flex-direction: row;
}
.left{
  margin-right: 16px;
  overflow: hidden;
  border-radius: 3px;
  display: flex;
  flex-direction: column;
  justify-content: center;
}
.leftImg{
  width: 100%;
  height: 100%;
}
.right{
  width: calc(100% - 130px);
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.rightMain{
  width: 100%;
  font-size: 16px;
  color: #1c2438;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.rightSub{
  width: 100%;
  font-size: 13px;
  color: #80848F;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.rightBottom{
  width: 100%;
  font-size: 11px;
  color: #80848F;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.rightBottomImg{
  width: 13px;
  height: 13px;
  vertical-align: middle;
  position: relative;
  top: -2px;
  margin-right: 3px;
}
.tabs{
  color:#fff;
  background:#0192D6;
  border:1px solid #0192D6;
  font-size:11px;
  border-radius:5px;
  padding:1px 5px;
  margin-right:5px;
  display: inline-block;
}
.tabs:last-child{
  margin: 0;
}